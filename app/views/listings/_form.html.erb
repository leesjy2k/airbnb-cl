<%= form_for @listing do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %><br />
  <%= form.label :description %>
  <%= form.text_field :description %><br />
  <%= form.label :price %>
  <%= form.number_field :price %><br />
  <%= form.label :location %>
  <%= form.text_field :location %><br />
  <%= form.label :bedroom %>
  <%= form.number_field :bedroom %><br />
  <%= form.file_field :photos, multiple: true %>
  <%= form.submit %>
<% end %>


